{
    "name": "nadeolive/maniadirect",
    "description": "ManiaDirect",
    "type": "project",
    "autoload": {
        "psr-4": { "Nadeo\\Live\\ManiaDirect\\": "src/" },
        "classmap": [ "app/ManiaDirect.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Nadeo\\Live\\ManiaDirect\\Tests\\": "tests/"}
    },
    "require": {
        "symfony/console": "~3.0",
        "symfony/process": "~3.0",
        "symfony/event-dispatcher": "~3.0",
        "symfony/stopwatch": "~3.0",
        "maniaplanet/dedicated-server-api": "~5.0@dev",
        "rdlowrey/auryn": "^1.4",
        "monolog/monolog": "^1.21"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "bin": [
        "bin/maniadirect.php"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
